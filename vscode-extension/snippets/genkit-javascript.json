      "const { ${1:claude} } = require('@genkit-ai/${2:anthropic}');",
      "",
      "module.exports = configureGenkit({",
      "  plugins: [",
      "    ${1:claude}({",
      "      apiKey: process.env.${3:ANTHROPIC_API_KEY},",
      "    }),",
      "  ],",
      "  logLevel: '${4:debug}',",
      "  enableTracingAndMetrics: ${5:true},",
      "});"
    ],
    "description": "Create Genkit configuration (JavaScript)"
  }
}
