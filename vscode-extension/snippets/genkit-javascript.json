      "",
      "exports.${1:flowName}Flow = defineFlow(",
      "  {",
      "    name: '${1:flowName}',",
      "    inputSchema: z.object({",
      "      ${2:input}: z.string(),",
      "    }),",
      "    outputSchema: z.${3:string}(),",
      "  },",
      "  async (input) => {",
      "    ${4:// Implement flow logic}",
      "    return ${5:input.${2:input}};",
      "  }",
      ");"
    ],
    "description": "Create a new Genkit flow (JavaScript)"
  },
  "Genkit Config": {
